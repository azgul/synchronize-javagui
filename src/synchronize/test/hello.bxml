<menus:MenuBars title="Synchronize" maximized="true"
    styles="{padding:{top:0, left:4, bottom:4, right:4}, showWindowControls:false}"
    xmlns:bxml="http://pivot.apache.org/bxml"
    xmlns:content="org.apache.pivot.wtk.content"
    xmlns:menus="synchronize.test"
    xmlns="org.apache.pivot.wtk">
    <bxml:define>
        <FileBrowserSheet bxml:id="fileBrowserSheet"/>
    </bxml:define>

    <menuBar>
        <MenuBar>
            <MenuBar.Item action="file">
                <buttonData>
            		<content:MenuItemData text="File"/>
            	</buttonData>
            </MenuBar.Item>

            <MenuBar.Item action="synchronize">
            	<buttonData>
            		<content:MenuItemData text="Sync"/>
            	</buttonData>
            </MenuBar.Item>

            <MenuBar.Item action="help">
            	<buttonData>
            		<content:MenuItemData text="Help"/>
            	</buttonData>
            </MenuBar.Item>

            <MenuBar.Item action="about">
            	<buttonData>
            		<content:MenuItemData text="About"/>
            	</buttonData>
            </MenuBar.Item>
        </MenuBar>
    </menuBar>
    
    <BoxPane styles="{padding:4, verticalAlignment:'center'}">
	    <ImageView image="hp800booster-movie_topbanner_gb.jpg"/>
    </BoxPane>
    
	<Border styles="{backgroundColor:null, padding:2}">
		<TabPane bxml:id="tabPane"/>
	    <!--BoxPane styles="{padding:{left:5,top:5,bottom:10,right:10}, horizontalAlignment:'right', verticalAlignment:'bottom'}">
			<Border bxml:id="backBorder" styles="{cornerRadii:0,padding:0,color:'#000000',thickness:2}">
				<PushButton bxml:id="back" buttonData="Go Back &lt;-" styles="{backgroundColor:'#FFFFFF',borderColor:'0xFFFFFF00',padding:{left:8,right:8,top:5,bottom:5}}">
				</PushButton>
			</Border>
		</BoxPane-->
	</Border>
    
</menus:MenuBars>